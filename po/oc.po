msgid "Preferences"
msgstr "Preferéncias"

#: src/ui/menu.ui:10
msgid "Shortcuts"
msgstr "Acorchis de clavièr"

#: src/ui/menu.ui:15
msgid "What's new?"
msgstr "Qué de nòu ?"

#: src/ui/menu.ui:19
msgid "Install GNOME extension"
msgstr "Installar l’extension GNOME"

#: src/ui/menu.ui:23
msgid "Translate this app"
msgstr "Traduire aquesta app"

#: src/ui/menu.ui:26
msgid "About"
msgstr "A prepaus"

